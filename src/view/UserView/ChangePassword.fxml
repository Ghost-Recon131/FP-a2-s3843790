<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserFeatures.ChangePasswordController">
   <Button layoutX="1132.0" layoutY="37.0" mnemonicParsing="false" onAction="#setLogoutButtonClick" prefHeight="33.0"
           prefWidth="88.0" text="Log Out">
      <font>
         <Font size="16.0"/>
      </font>
   </Button>
   <Button layoutX="1030.0" layoutY="37.0" mnemonicParsing="false" onAction="#setBackButtonClick" prefHeight="33.0"
           prefWidth="88.0" text="Back">
      <font>
         <Font size="16.0"/>
      </font>
   </Button>
   <Text layoutX="464.0" layoutY="131.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password Management">
      <font>
         <Font name="System Bold" size="32.0"/>
      </font>
   </Text>
   <Text layoutX="297.0" layoutY="202.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Current Password: ">
      <font>
         <Font size="20.0"/>
      </font>
   </Text>
   <Text layoutX="298.0" layoutY="248.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter New Password: ">
      <font>
         <Font size="20.0"/>
      </font>
   </Text>
   <Text layoutX="297.0" layoutY="300.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm New Password: ">
      <font>
         <Font size="20.0"/>
      </font>
   </Text>
   <Button layoutX="516.0" layoutY="327.0" mnemonicParsing="false" onAction="#setUpdatePasswordButtonClick"
           prefHeight="35.0" prefWidth="147.0" text="Update Password">
      <font>
         <Font size="16.0"/>
      </font>
   </Button>
   <Button layoutX="666.0" layoutY="327.0" mnemonicParsing="false" onAction="#setBackButtonClick" prefHeight="35.0"
           prefWidth="89.0" text="Cancel">
      <font>
         <Font size="16.0"/>
      </font>
   </Button>
   <Label fx:id="PasswordError" layoutX="771.0" layoutY="179.0" prefHeight="30.0" prefWidth="301.0" text=" "
          textFill="RED">
      <font>
         <Font size="16.0"/>
      </font>
   </Label>
   <Label fx:id="NewPasswordError" layoutX="771.0" layoutY="229.0" prefHeight="30.0" prefWidth="327.0" text=" "
          textFill="RED">
      <font>
         <Font size="16.0"/>
      </font>
   </Label>
   <Label fx:id="ConfirmPasswordError" layoutX="771.0" layoutY="280.0" prefHeight="30.0" prefWidth="327.0" text=" "
          textFill="RED">
      <font>
         <Font size="16.0"/>
      </font>
   </Label>
   <Label fx:id="ChangePasswordError" layoutX="771.0" layoutY="330.0" prefHeight="30.0" prefWidth="295.0" text=" "
          textFill="RED">
      <font>
         <Font size="16.0"/>
      </font>
   </Label>
   <PasswordField fx:id="Password" layoutX="514.0" layoutY="180.0" prefHeight="30.0" prefWidth="242.0"/>
   <PasswordField fx:id="NewPassword" layoutX="514.0" layoutY="229.0" prefHeight="30.0" prefWidth="242.0"/>
   <PasswordField fx:id="ConfirmPassword" layoutX="515.0" layoutY="280.0" prefHeight="30.0" prefWidth="242.0"/>
</AnchorPane>
