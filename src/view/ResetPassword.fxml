<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ResetPasswordController">
   <Button fx:id="HomeButton" layoutX="1103.0" layoutY="42.0" mnemonicParsing="false" onAction="#setHomeButtonClick"
           prefHeight="46.0" prefWidth="122.0" text="Home">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
   <Text layoutX="304.0" layoutY="241.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Your Secret Question">
      <font>
         <Font size="20.0"/>
      </font>
   </Text>
   <TextField fx:id="SQ" layoutX="553.0" layoutY="215.0" prefHeight="37.0" prefWidth="332.0">
      <font>
         <Font size="16.0"/>
      </font>
   </TextField>
   <Text layoutX="528.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Forgot Pasword">
      <font>
         <Font name="System Bold" size="30.0"/>
      </font>
   </Text>
   <PasswordField fx:id="SQ_A" layoutX="552.0" layoutY="272.0" prefHeight="37.0" prefWidth="332.0">
      <font>
         <Font size="16.0"/>
      </font>
   </PasswordField>
   <Text layoutX="304.0" layoutY="298.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Answer to Secret Question">
      <font>
         <Font size="20.0"/>
      </font>
   </Text>
   <Label fx:id="SQError" layoutX="893.0" layoutY="218.0" prefHeight="30.0" prefWidth="348.0" text="   " textFill="RED">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Button fx:id="ResetPasswordButton" layoutX="557.0" layoutY="324.0" mnemonicParsing="false"
           onAction="#setResetPasswordButtonClick" prefHeight="30.0" prefWidth="184.0" text="Reset Password">
      <font>
         <Font size="22.0"/>
      </font>
   </Button>
   <Label fx:id="ErrorMessage" layoutX="781.0" layoutY="331.0" prefHeight="30.0" prefWidth="401.0" text="   "
          textFill="RED">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label fx:id="SQAError" layoutX="893.0" layoutY="275.0" prefHeight="30.0" prefWidth="348.0" text="   "
          textFill="RED">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label fx:id="SuccessLabel" layoutX="407.0" layoutY="337.0" prefHeight="66.0" prefWidth="484.0"
          text="Password Reset Successfully" visible="false">
      <font>
         <Font name="System Bold" size="36.0"/>
      </font>
   </Label>
   <TextField fx:id="Password" alignment="CENTER" editable="false" layoutX="462.0" layoutY="427.0" prefHeight="83.0"
              prefWidth="374.0" promptText="Password" visible="false">
      <font>
         <Font size="18.0"/>
      </font>
   </TextField>
   <Label fx:id="Success2" contentDisplay="CENTER" layoutX="462.0" layoutY="530.0" prefHeight="32.0" prefWidth="374.0"
          text="Please note it down and keep it safe :)" visible="false">
      <font>
         <Font size="22.0"/>
      </font>
   </Label>
</AnchorPane>
